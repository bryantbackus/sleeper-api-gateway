{
  "name": "sleeper-api-middleware",
  "version": "1.0.0",
  "description": "API middleware server for Sleeper fantasy football API with caching and authentication",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest --config tests/automated/jest.config.js",
    "test:watch": "jest --config tests/automated/jest.config.js --watch",
    "test:coverage": "jest --config tests/automated/jest.config.js --coverage",
    "test:comprehensive": "jest --config tests/automated/jest.config.js tests/automated/comprehensive.test.js",
    "test:all": "npm run test:coverage && npm run test:postman",
    "test:postman": "newman run tests/postman/Sleeper_API_Middleware_Consolidated.postman_collection.json -e tests/postman/Consolidated_Environment.json --reporters cli",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "setup": "node scripts/manage.js",
    "setup:docker": "node scripts/docker-setup.js",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:restart": "docker-compose restart",
    "docker:clean": "docker-compose down -v --remove-orphans && docker system prune -f",
    "ssl:generate": "node scripts/ssl-manager.js generate",
    "ssl:renew": "node scripts/ssl-manager.js renew",
    "ssl:status": "node scripts/ssl-manager.js status",
    "ssl:enable": "node scripts/ssl-manager.js enable",
    "ssl:disable": "node scripts/ssl-manager.js disable",
    "deploy": "npm run setup:docker",
    "quick-start": "npm run setup && npm install && npm start"
  },
  "keywords": [
    "sleeper",
    "fantasy-football",
    "api",
    "middleware",
    "nodejs",
    "express"
  ],
  "author": "Bryant Backus",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "moment-timezone": "^0.5.43",
    "node-cron": "^3.0.3",
    "sqlite3": "^5.1.6",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "newman": "^6.0.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
